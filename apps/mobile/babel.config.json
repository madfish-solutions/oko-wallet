{
  "presets": [
    "module:metro-react-native-babel-preset"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "ui": "../../libs/ui/src"
        }
      }
    ],
    "@babel/plugin-proposal-export-namespace-from",
    [
      "react-native-reanimated/plugin",
      {
        "globals": ["__scanCodes"]
      }
    ]
  ]
}
